<!DOCTYPE html>
<html>
<%- include("./partials/head", {title:'Krak Miami - Consultas'})  %>

<body>
  <!-- Navbar -->
  <%- include("./partials/navbar") %>

  <!-- Lista de Consultas -->
  <main class="consultas-box">
    <div class="container-sm">
      <h2>Consultas</h2>
      <table>
        <tr class="th">
          <th>Nombre</th>
          <th>Email</th>
          <th>Telefono</th>
          <th>Fecha</th>
          <th>Mensaje</th>
          <th>âœ”</th>
          <th> </th>
        </tr>

        <% for (i=0 ; i < consulta.length ; i++) { %>
        <tr>
          <td><%= consulta[i].Nombre %></td>
          <td><%= consulta[i].Email %></td>
          <td><%= consulta[i].Telefono %></td>
          <td><%= consulta[i].created_at %></td>
          <td><%= consulta[i].Contenido %></td>
          <td><input type="checkbox" class="check"></td>
          <td class="remove"><div>&times</div></td>
        </tr>
        <% } %>

      </table>
    </div>
  </main>
  <!-- Footer -->
  <%- include("./partials/footer") %>

  <!-- Scripts -->
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM" crossorigin="anonymous"></script>
  <script src="../javascripts/index.js"></script>
</body>

</html>